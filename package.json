{
  "name": "halo-score-board",
  "version": "1.0.0",
  "description": "Track daily halo score",
  "main": "server.js",
  "scripts": {
    "start": "node api/server.js",
    "server": "nodemon api/server.js",
    "client": "npm start --prefix client",
    "json-server": "json-server -p 4000 ./api/data/db.json",
    "local": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ayemane/spacex_launch_missions.git"
  },
  "keywords": [
    "react",
    "apollo",
    "graphql"
  ],
  "author": "Akilas Yemane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayemane/halo-score-board/issues"
  },
  "homepage": "https://github.com/ayemane/halo-score-board#readme",
  "dependencies": {
    "aws-sdk": "^2.453.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4",
    "json-server": "^0.14.2",
    "moment": "^2.24.0",
    "mongodb": "^3.2.4",
    "mysql": "latest",
    "mysql-model": "0.0.3",
    "mysql2": "^1.6.5",
    "sequelize": "^5.8.6",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0"
  }
}
